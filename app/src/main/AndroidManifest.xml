<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.picasso.customview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".bezier.ui.SecondOrderBezierActivity"
            android:label="@string/second_bezier" />
        <activity
            android:name=".bezier.ui.ThreeOrderBezierActivity"
            android:label="@string/three_bezier" />
        <activity
            android:name=".bezier.ui.SlipBezierActivity"
            android:label="@string/clipboard" />
        <activity
            android:name=".bezier.ui.Clean360Activity"
            android:label="@string/clean_360"></activity>
    </application>

</manifest>